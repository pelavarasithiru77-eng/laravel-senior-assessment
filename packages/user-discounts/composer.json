{
  "name": "vendor/user-discounts",
  "description": "Laravel package to manage user discounts",
  "type": "library",
  "license": "MIT",

  "authors": [
    {
      "name": "Elavarasi",
      "email": "pelavarasibca77@gmail.com"
    }
  ],

  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },

  "autoload": {
    "psr-4": {
      "Vendor\\UserDiscounts\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Vendor\\UserDiscounts\\Tests\\": "tests/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "Vendor\\UserDiscounts\\UserDiscountServiceProvider"
      ]
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}
